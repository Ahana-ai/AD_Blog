<%- include('layouts/header') %>


    <body id="page-top">

        <!-- Page Wrapper -->
        <div id="wrapper">

            <%- include('layouts/sidebar') %>


                <!-- Content Wrapper -->
                <div id="content-wrapper" class="d-flex flex-column">

                    <!-- Main Content -->
                    <div id="content">

                        <%- include('layouts/topnav') %>


                            <!-- Begin Page Content -->
                            <div class="container-fluid">

                                <!-- Page Heading -->
                                <div class="d-sm-flex align-items-center justify-content-between mb-4">
                                    <h1 class="h3 mb-0 text-gray-800">Contact</h1>
                                </div>

                                <!--Form Begin-->

                                <div class="container">
                                    <div class="card" style="width: 50rem;">
                                        <div class="card-body">

                                            <p class="card-text">
                                            <div class="row ">
                                                <div class="col-lg-12 mx-auto">
                                                    <div class="card mt-2 mx-auto p-4 bg-light">
                                                        <div class="card-body bg-light">

                                                            <div class="container">
                                                                <form action="/admin/contact-update" method="post"
                                                                    id="contact-form" role="form">
                                                                    <div class="controls">
                                                                        <div class="row">
                                                                            <div class="col-md-12">
                                                                                <div class="form-group">
                                                                                    <label for="form_name">First Name
                                                                                    </label>
                                                                                    <input id="firstName" type="text" value="<%= response.firstName %>"
                                                                                        name="firstName" class="form-control" readonly
                                                                                        data-error="First Name is required.">
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <div class="row">
                                                                            <div class="col-md-12">
                                                                                <div class="form-group">
                                                                                    <label for="form_name">Last Name
                                                                                    </label>
                                                                                    <input id="text" type="lastName"
                                                                                        name="lastName" value="<%= response.lastName %>"
                                                                                        required="required" class="form-control"
                                                                                        data-error="Last Name is required." readonly>

                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <div class="row">
                                                                            <div class="col-md-12">
                                                                                <div class="form-group">
                                                                                    <label for="form_name">Message
                                                                                    </label>
                                                                                    <input id="message" type="text"
                                                                                        name="message" value="<%= response.message %>"
                                                                                        class="form-control"
                                                                                        required="required"
                                                                                        data-error="Writer is required.">

                                                                                </div>
                                                                            </div>
                                                                            <!-- <div class="row">
                                                                                <div class="col-md-12">
                                                                                    <div class="form-group">
                                                                                        <label for="form=">Status
                                                                                        </label>
                                                                                        <input id="status" type="radio"
                                                                                            name="status" value="<%= response.status %>"
                                                                                            
                                                                                            required="required"
                                                                                            data-error="Status is required." checked>
                                                                                        <label for="active">Active</label>
                                                                                        <input id="status" type="radio"
                                                                                            name="status" value="<%= response.status %>"
                                                                                            
                                                                                            required="required"
                                                                                            data-error="Status is required.">
                                                                                        <label for="offline">Offline</label>
                                                                                    </div>
                                                                                </div>
                                                                            </div> -->
                                                                        </div>

                                                                        <div class="row">
                                                                            <div class="col-md-12">
                                                                                <input type="hidden" name="id" value="<%= response._id%>">
                                                                                <input type="submit" class="btn btn-success btn-send  pt-2 btn-block
                                                                      " value="Edit">

                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </form>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            </p>

                                        </div>
                                    </div>
                                </div>


                                <!--Form end-->
                            </div>
                            <!-- /.container-fluid -->

                    </div>
                    <!-- End of Main Content -->


                    <%- include('layouts/siteFooter') %>

                </div>
                <!-- End of Content Wrapper -->

        </div>
        <!-- End of Page Wrapper -->

        <!-- Scroll to Top Button-->
        <a class="scroll-to-top rounded" href="#page-top">
            <i class="fas fa-angle-up"></i>
        </a>


        <%- include('layouts/footer') %>